<h1>Accounts#edit</h1>
<p>Find me in app/views/accounts/edit.html.erb</p>

<%= form_for @account, html: { class: "form-inline", role: "form" } do |f| %>
  <div class="form-group">
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Update Account', class: 'btn btn-default' %>
  </div>
<% end %>

<p>
  <%= link_to "Cancel", @account %>
</p>
