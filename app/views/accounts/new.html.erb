<h1>Accounts#new</h1>
<p>Find me in app/views/accounts/new.html.erb</p>

<%= form_for @account, html: { class: "form-inline", role: "form" } do |f| %>
  <div class="form-group">
    <%= f.text_field :name, class: 'form-control', placeholder: 'Account Name' %>
  </div>
  <div class="form-group">
    <%= f.submit 'Create Account', class: 'btn btn-default' %>
  </div>
<% end %>
